{
  "manifest_version": 2,

  "name": "Save It for Later",
  "description": "Sign in with your Save it account from your mobile app and start sendig links to your phone",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://www.gstatic.com/*"
  ],
  "content_scripts": [
    {
      "js": []
    }
  ]
}